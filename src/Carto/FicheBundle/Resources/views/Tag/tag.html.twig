{% extends '::base.html.twig' %}
{%block body%}
    <h2>Liste des tags</h2>

    <table>
        <thead>
        <tr>
            <th>{{ 'Libelle' }}</th>
            <th>{{ 'Modifier' }}</th>
        </tr>
        </thead>
        <tbody>
        {% for tag in tags %}
            <tr>
                <td>{{ tag.libelle }}</td>
                <td class="center"><a href="{{ path('carto_fiche_tag_edit', { 'id': tag.id }) }}">{{ 'Modifier' }}</a></td>
            </tr>

        {% endfor %}
        </tbody>
    </table>
{%endblock%}